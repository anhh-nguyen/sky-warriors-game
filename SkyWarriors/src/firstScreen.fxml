    <stylesheets>
        <URL value="@backgound.css" />
    </stylesheets>
   <children>
      <Button fx:id="startBtn" layoutX="244.0" layoutY="377.0" mnemonicParsing="false" onAction="#onStartBtn" scaleX="2.0" scaleY="2.0" style="-fx-background-color: white; -fx-font-weight: bold; -fx-text-fill: gray; -fx-border-radius: 40; -fx-background-radius: 20; -fx-border-color: gray;" text="Start Game" />
      <Button fx:id="exitBtn" layoutX="518.0" layoutY="377.0" mnemonicParsing="false" onAction="#onExitBtn" scaleX="2.0" scaleY="2.0" style="-fx-background-color: white; -fx-font-weight: bold; -fx-text-fill: gray; -fx-border-radius: 40; -fx-background-radius: 20; -fx-border-color: gray;" text="Exit" />
   </children>
