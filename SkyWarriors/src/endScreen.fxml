<AnchorPane fx:id="root" prefHeight="608.0" prefWidth="867.0" styleClass="endPage" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="the.SkyWarriors.EndScreenController">
