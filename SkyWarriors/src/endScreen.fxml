        <URL value="@backgound.css"/>
    </stylesheets>
    <children>
        <Button fx:id="startBtn" layoutX="271.0" layoutY="306.0" mnemonicParsing="false" onAction="#onStartBtn" scaleX="2.0" scaleY="2.0" style="-fx-background-color: white; -fx-font-weight: bold; -fx-text-fill: gray; -fx-border-radius: 40; -fx-background-radius: 20; -fx-border-color: gray;" text="Start Over" />
        <Button fx:id="exitBtn" layoutX="569.0" layoutY="307.0" mnemonicParsing="false" onAction="#onExitBtn" scaleX="2.0" scaleY="2.0" style="-fx-background-color: white; -fx-font-weight: bold; -fx-text-fill: gray; -fx-border-radius: 40; -fx-background-radius: 20; -fx-border-color: gray;" text="Exit Game" />
      <Rectangle fx:id="rec" arcHeight="5.0" arcWidth="10.0" fill="WHITE" height="128.0" layoutX="145.0" layoutY="429.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="10.0" width="652.0" />
      <Label fx:id="staticEnemies" layoutX="211.0" layoutY="452.0" text="Enemies Killed:" />
      <Label fx:id="staticMoney" layoutX="534.0" layoutY="450.0" text="Leftover Money:" />
      <Label fx:id="staticTowers" layoutX="543.0" layoutY="496.0" text="Towers Placed: " />
      <Label fx:id="enemieskilledLabel" layoutX="324.0" layoutY="452.0" text="Label" />
      <Label fx:id="leftmoneyLabel" layoutX="649.0" layoutY="452.0" prefHeight="19.0" prefWidth="65.0" text="Label" />
      <Label fx:id="towerplacedLabel" layoutX="650.0" layoutY="496.0" text="Label" />
    </children>
</AnchorPane>
